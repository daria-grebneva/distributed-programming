@{
    ViewData["Title"] = "Upload Your File";
}
<div class="text-left">
    <h1 class="display-4">input form</h1>
    <form method="post">
        <input name="data" type="text"/><br>
         <select name="region">
            <option value="rus">RUS</option>
            <option value="eu">EU</option>
            <option value="us">US</option>
        </select>
        <input class="btn mt-3" type="submit" value="send message">
    </form>
</div>
